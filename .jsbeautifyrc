{
    "html": {
        "allowed_file_extensions": ["html"],
        "indent_size": "2",
        "indent_char": " ",
        "max_preserve_newlines": "-1",
        "preserve_newlines": false,
        "brace_style": "collapse",
        "end_with_newline": false,
        "wrap_line_length": "0",
        "indent_inner_html": true,
        "indent_empty_lines": false,
        "unformatted": ["svg", "pre"]
    }
}
